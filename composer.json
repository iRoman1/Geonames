{
  "name": "michaeldrennen/geonames",
  "description": "A Laravel (php) package that interfaces with the geolocation services on geonames.org.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Drennen",
      "email": "michaeldrennen74@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0 || ^8.1 || ^8.2",
    "ext-intl": "*",
    "ext-zip": "*",
    "laravel/framework": "^8.0 || ^9.0 || ^10.0 || ^11.0",
    "michaeldrennen/remote-file": "^2.0",
    "michaeldrennen/local-file": "^2.0",
    "curl/curl": "^2.2",
    "symfony/browser-kit": "^6.0",
    "symfony/http-client": "^5.4|^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0 || ^10.1",
    "orchestra/testbench": "^6.0 || ^7.0 || ^8.0 || ^9.0",
    "php-coveralls/php-coveralls": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "MichaelDrennen\\Geonames\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MichaelDrennen\\Geonames\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "MichaelDrennen\\Geonames\\GeonamesServiceProvider"
      ]
    }
  }
}
